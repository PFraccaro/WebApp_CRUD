@model ICollection<Employees>

@{
    ViewBag.Title = "title";
    Layout = "_Layout";
}

<h2>Employees</h2>

<table>
    <thead>
    <tr>
        <th>ImiÄ™</th>
        <th>Nazwisko</th>
        <th>Operations</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var employee in Model)
    {
        <tr>
            <td>@employee.FirstName</td>
            <td>@employee.LastName</td>
            <td>
                <a asp-action="Edit" asp-route-id="@employee.EmployeeId" class="btn btn-primary">
                    Edycja
                </a>
            </td>
        </tr>
    }
    </tbody>
</table>